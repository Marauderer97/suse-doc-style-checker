<?xml version="1.0" encoding="UTF-8"?>
<terminology ignoredwords="([b-z0-9].*)" useonepattern="no">

  <term>
    <accept>
      <word>and</word>
    </accept>
    <!-- "an more" could also be "any more"/"anymore"... -->
    <patterngroup>
      <pattern1>an</pattern1>
      <contextpattern>(more|most|some|many|few)</contextpattern>
    </patterngroup>
    <patterngroup>
      <pattern1>an[sf]</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>interest</word>
    </accept>
    <patterngroup>
      <pattern1>inte(ress|rres|rress)ts?</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>interest</word>
    </accept>
    <patterngroup>
      <pattern1>inte(ress|rres|rress)ted</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>interesting</word>
    </accept>
    <patterngroup>
      <pattern1>inte(ress|rres|rress)ting</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>interestingly</word>
    </accept>
    <patterngroup>
      <pattern1>inte(ress|rres|rress)tingly</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>relevant</word>
    </accept>
    <patterngroup>
      <pattern1>relav[ae]nt</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>separate</word>
    </accept>
    <patterngroup>
      <pattern1>seperate</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>separately</word>
    </accept>
    <patterngroup>
      <pattern1>seperately</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>separation</word>
    </accept>
    <patterngroup>
      <pattern1>seperations?</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>separate</word>
    </accept>
    <patterngroup>
      <pattern1>seperat(e[ds]?|ing)</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>solution</word>
    </accept>
    <patterngroup>
      <pattern1>soultions?</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>since</word>
    </accept>
    <patterngroup>
      <pattern1>sicne</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>maybe</word>
    </accept>
    <patterngroup>
      <pattern1>(ma?yby|mybe)</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>other</word>
    </accept>
    <patterngroup>
      <pattern1>(ohter|otehr)</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>the</word>
    </accept>
    <patterngroup>
      <pattern1>teh</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>weight</word>
    </accept>
    <patterngroup>
      <pattern1>weigths?</pattern1>
    </patterngroup>
  </term>

  <term>
    <accept>
      <word>width</word>
    </accept>
    <patterngroup>
      <pattern1>widhts?</pattern1>
    </patterngroup>
  </term>

</terminology>