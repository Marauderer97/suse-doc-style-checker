    <matchgroup>
      <match>appendices$</match>
    </matchgroup>
  </term>
  <term>
    <accept>[Aa]udio CD</accept>
    <matchgroup>
      <match>audio[\-_]?cd</match>
    </matchgroup>
    <matchgroup>
      <match>cd[\-_]?audio</match>
    </matchgroup>
    <matchgroup>
      <match>cd[\-_]?audio</match>
    </matchgroup>
  </term>
  <term>
    <accept>[Bb]ack-ends?$</accept>
    <matchgroup>
      <match>backends?$</match>
    </matchgroup>
  </term>
  <term>
    <accept>[Bb]ack(ed|s|ing)? up</accept>
    <matchgroup>
      <match>back(ed|s|ing)-?up</match>
    </matchgroup>
    <matchgroup>
      <match>back-?upp?(ed|s|ing)</match>
    </matchgroup>
    <matchgroup>
      <match>back-?up</match>
      <context type="previous">(when|to)</context>
    </matchgroup>
    <matchgroup>
      <match>back-?up</match>
      <context type="next">(them|these|this|it|the|a|an)</context>
    </matchgroup>
  </term>
  <term>
    <accept>backup</accept>
    <matchgroup>
      <match>back-ups?</match>
      <context type="previous">(the|a|local|remote|system|full|partial|automatic|manual)</context>
    </matchgroup>
    <matchgroup>
      <match>back</match>
      <context type="next">ups?</context>
      <context type="previous">(the|a|local|remote|system|full|partial|automatic|manual)</context>
    </matchgroup>
  </term>
  <term>
    <accept>Bash$</accept>
    <matchgroup>
      <match>bash$</match>
    </matchgroup>
  </term>
  <term>
    <accept/>
    <matchgroup>
      <match>basically$</match>
    </matchgroup>
  </term>
  <term>
    <accept>because of</accept>
    <matchgroup>
      <match>(due|owing)$</match>
      <context type="next">to$</context>
    </matchgroup>
  </term>
  <term>
    <accept>boot disks?</accept>
    <matchgroup>
      <match>boot-?disks?</match>
    </matchgroup>
  </term>
  <term>
