  <term>
    <accept>[Aa]dd-ons?</accept>
    <matchgroup>
      <match>add[-_]?ons?</match>
    </matchgroup>
    <matchgroup>
      <match>add</match>
      <context type="next">ons?</context>
    </matchgroup>
