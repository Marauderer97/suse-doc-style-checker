  <term>
    <accept>[Aa]ddress books?</accept>
    <matchgroup>
      <match>addressbooks?</match>
    </matchgroup>
  </term>
  <term>
    <accept>[Aa]dvice$</accept>
    <matchgroup>
      <match>advise</match>
      <context type="previous">(an|the)</context>
    </matchgroup>
  </term>
  <term>
    <accept>[Aa]dvisor(y|ies)</accept>
    <matchgroup>
      <match>advicor(y|ies)</match>
    </matchgroup>
  </term>
  <term>
    <accept>advis(e[sd]|ing)</accept>
    <matchgroup>
      <match>advic(e[sd]|ing)</match>
    </matchgroup>
    <matchgroup>
      <match>advice</match>
      <context type="previous">(we|suse|novell|attachmate|[\-a-z]*?ly)</context>
    </matchgroup>
  </term>
  <term>
    <accept>[Aa]fter</accept>
    <matchgroup>
      <match>once</match>
      <context type="next">(the|a|an|it|they)</context>
    </matchgroup>
  </term>
