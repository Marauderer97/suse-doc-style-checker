  <term>
    <accept>check box(es)</accept>
    <matchgroup>
      <match>(check|tick)-?box(es)?</match>
    </matchgroup>
    <matchgroup>
      <match>tick</match>
      <context type="next">box(es)?</context>
    </matchgroup>
  </term>
  <term>
