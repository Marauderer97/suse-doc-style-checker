<?xml version="1.0" encoding="UTF-8"?>
<terminology id="terms1">
<!-- Make sure to change the id when creating a new terminology file. -->
<!-- before/after: within sentence, previous/next: immediate -->
  <term>
    <accept>32-bit</accept>
    <matchgroup>
      <match>32[\.\- _]?bit</match>
    </matchgroup>
  </term>
  <term>
    <accept>3D</accept>
    <matchgroup>
      <match>(three|3)[\.\-_]?[ ]?d[\.]?</match>
    </matchgroup>
  </term>
  <term>
    <accept>64-bit</accept>
    <matchgroup>
      <match>64[\.\- _]?bit</match>
    </matchgroup>
  </term>
