    </matchgroup>
  </term>
  <term>
    <accept>boot (using|via) PXE</accept>
    <matchgroup>
      <match>PXE</match>
      <context type="next">boot(ing|ed)</context>
    </matchgroup>
    <matchgroup>
      <match>PXE</match>
      <context type="next">boot</context>
      <context type="previous">(to|not)</context>
    </matchgroup>
  </term>
  <term>
    <accept>Btrfs</accept>
    <matchgroup>
      <match>b[eu]tterfs</match>
    </matchgroup>
    <matchgroup>
      <match>b[eu]tter</match>
      <context type="next">fs</context> <!-- Check for written-out file system, too? -->
    </matchgroup>
    <matchgroup>
      <match>b\.?t\.?r\.?f\.?s\.?</match>
    </matchgroup>
  </term>
